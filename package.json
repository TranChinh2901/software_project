{
  "name": "express-app",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed-users-roles.ts",
    "test:api": "ts-node -r tsconfig-paths/register src/scripts/test-api.ts",
    "seed:blog": "ts-node -r tsconfig-paths/register src/scripts/seed-blog.ts",
    "dev": "nodemon --watch ./src --ext ts --exec ts-node -r tsconfig-paths/register src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/config/database.config.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/database.config.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/config/database.config.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "Backend API cho website bán quần áo sử dụng Express với TypeScript và TypeORM.",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.14.1",
    "nodemon": "^3.1.9",
    "reflect-metadata": "^0.1.14",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nhom7-NodeJS/MockProject_062025_Nhom7.git"
  },
